package mms.member.vo;

//2. 회원 한명의 정보를 저정하는 클래스
public class Member {
	
	private int id;
	private String name; //식별값
	private String addr;
	private String nation;
	private String email;
	private int age;
	
	public Member(int id, String name, String addr, String nation, String email, int age) {
		this.id = id;
		this.name = name;
		this.addr = addr;
		this.nation = nation;
		this.email = email;
		this.age = age;
	}
	
}

